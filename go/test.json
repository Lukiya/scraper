{
    "data": {
        "rootURL": "https://www.xhwx3.com",
        "searchURL": "https://www.xhwx3.com/search/",
        "sel_s_token": "input[name='_token']",
        "sel_s_list": "",
        "sel_s_list_name": "",
        "sel_s_list_url": "",
        "sel_s_list_author": "",
        "sel_s_list_image": "",
        "name": "灵境行者",
        "doc_home": "%GOQUERY DOC% ",
        "token": "xxxxxxxxxxxxxxxxx",
        "doc_search": "%GOQUERY DOC% ",
        "sel_s_list_name_v": "",
        "sel_s_list_url_v": "",
        "sel_s_list_author_v": "",
        "sel_s_list_image_v": "",
        "books": "BOOK MAP LIST"
    },
    "steps": [
        {
            "Method": "GET->{rootURL}",
            "Output": "doc_home"
        },
        {
            "Method": "TEXT->{doc_home},{sel_search_token}",
            "Output": "token"
        },
        {
            "Method": "POST->{searchURL},_token={token}&kw={name}",
            "Output": "doc_search"
        },
        {
            "Method": "LIST->{doc_search}",
            "Each": {
                "sel_s_list_name_v": "TEXT->{sel_s_list_name}"
            },
            "Output": "books"
        }
    ]
}